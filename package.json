{
  "name": "Hackathon-ToDo",
  "description": "A bunch of code which handles core APIs of ToDo",
  "keywords": [
    "express",
    "mongoose",
    "mongodb"
  ],
  "version": "1.0.0",
  "author": "CodingKnights",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/mattaharish/hackathon-backend.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=10.11.0"
  },
  "scripts": {
    "start": "NODE_ENV=development nodemon server.js",
    "debug": "NODE_ENV=development nodemon --debug server.js",
    "prod": "pm2 start ecosystem.config.js --env production",
    "lint": "eslint --fix .",
    "format": "prettier-eslint $PWD --write"
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --fix .",
      "prettier-eslint --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "bcryptjs": "^2.3.0",
    "cors": "~2.8.5",
    "dayjs": "^1.8.15",
    "dotenv": "~7.0.0",
    "express": "~4.16.4",
    "express-list-endpoints": "^4.0.0",
    "jsonwebtoken": "^7.1.9",
    "lodash": "^4.15.0",
    "moment": "^2.26.0",
    "mongoose": "~5.7.5",
    "morgan": "~1.9.1",
    "pm2": "^4.4.0",
    "redis": "^3.0.2",
    "validator": "^11.0.0",
    "web-push": "^3.4.4",
    "winston": "~3.2.1",
    "winston-daily-rotate-file": "^3.9.0"
  },
  "devDependencies": {
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-node": "^4.0.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.0.10",
    "lint-staged": "^9.5.0",
    "nodemon": "*",
    "prettier": "^1.19.1",
    "prettier-eslint-cli": "^5.0.0",
    "supertest": "*"
  }
}
